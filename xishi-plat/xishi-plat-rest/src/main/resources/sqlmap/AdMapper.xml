<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cdnhxx.xishi.user.plat.dao.mapper.AdMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cdnhxx.xishi.plat.rest.model.pojo.Ad">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="cover" property="cover"/>
        <result column="content" property="content"/>
        <result column="content_type" property="contentType"/>
        <result column="click_times" property="clickTimes"/>
        <result column="size" property="size"/>
        <result column="location" property="location"/>
        <result column="state" property="state"/>
        <result column="start_time" property="startTime"/>
        <result column="end_time" property="endTime"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="admin_id" property="adminId"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, cover, content, content_type,click_times,size, location, state, start_time, end_time, create_time, update_time, admin_id
    </sql>

</mapper>
